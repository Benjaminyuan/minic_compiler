FUNCTION fibo :
  PARAM v8
  temp2 := #1
  IF v8 == temp2 GOTO label3
  GOTO label4
LABEL label4 :
  temp3 := #2
  IF v8 == temp3 GOTO label3
  GOTO label2
LABEL label3 :
  temp4 := #1
  RETURN temp4
LABEL label2 :
  temp5 := #1
  temp6 := v8 - temp5
  ARG temp6
  temp7 := CALL fibo
  temp8 := #2
  temp9 := v8 - temp8
  ARG temp9
  temp10 := CALL fibo
  temp11 := temp7 + temp10
  RETURN temp11
LABEL label1 :

FUNCTION main :
  temp12 := #0
  v11 := temp12
  temp13 := CALL read
  v11 := temp13
  temp14 := #1
  v13 := temp14
  temp15 := #1
  v13 := v13 + temp15
  temp16 := #a
  v14 := temp16
  temp17 := #1.000000
  v13 := v13 + temp17
  temp18 := #1.000000
  v13 := v13 + temp18
  temp19 := #1.000000
  v13 := v13 - temp19
LABEL label15 :
  IF v13 <= v11 GOTO label14
  GOTO label13
LABEL label14 :
  ARG v13
  temp20 := CALL fibo
  v12 := temp20
  ARG v12
  CALL  write
  temp22 := #1
  temp23 := v13 + temp22
  v13 := temp23
  GOTO label15
LABEL label13 :
LABEL label18 :
  temp24 := #1.000000
  v13 := v13 + temp24
  temp25 := #1
  RETURN temp25
LABEL label5 :
